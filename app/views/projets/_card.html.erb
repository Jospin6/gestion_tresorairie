<div id="<%= dom_id projet %>" class="text-[14px]">
  <p class="">
    <strong class="block font-medium mb-1">Titre:</strong>
    <%= projet.titre %>
  </p>

  <p class="">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= projet.description %>
  </p>

  <p class="">
    <strong class="block font-medium mb-1">Date debut:</strong>
    <%= projet.date_debut %>
  </p>

  <p class="">
    <strong class="block font-medium mb-1">Date fin:</strong>
    <%= projet.date_fin %>
  </p>

  <p>
    <%= link_to "Editer le projet", edit_projet_path(projet), class: "text-blue-500" %>
  </p>

</div>