<tr id="<%= dom_id partenaire %>" class="border-b-[1px] border-gray-400 text-center text-[14px]">
  <td>
    
    <% if partenaire.photo != nil %>
      <%= image_tag partenaire.photo.url, alt: 'partenaire', class: "h-[40px] w-[40px] rounded-full" %>
    <% end %>
    
  </td>
  <td><%= partenaire.nom %></td>
  <td><%= partenaire.type_partenaire %></td>
  <td><%= partenaire.contact %></td>
  <td><%= partenaire.mail %></td>
  <td><%= partenaire.adresse %></td>
  <td>
    <%= link_to "Voir", partenaire, class: "text-blue-500" %>
  </td>
</tr>
